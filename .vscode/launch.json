{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug (gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/SimpleLogo",
            "args": ["-i", "Test.txt", "-o", "Tested.txt", "-s", "100"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/bin/",
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        }
    ]
}
